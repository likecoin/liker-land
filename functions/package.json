{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint *.js",
    "clean": "rimraf 'dist/functions'",
    "build": "BABEL_ENV=production babel . --out-dir 'dist' --ignore 'nuxt,node_modules,dist'",
    "deps:copy": "cp -r nuxt.config.js package.json package-lock.json nuxt dist/",
    "deps:install": "cd 'dist' && npm install --production",
    "package": "npm run clean && npm run build && npm run deps:copy && npm run deps:install"
  },
  "dependencies": {
    "@likecoin/ui-vue": "^0.7.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/pwa": "^3.0.0-beta.10",
    "@nuxtjs/sentry": "^2.3.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "date-fns": "^1.30.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "firebase-admin": "^7.0.0",
    "firebase-functions": "^2.2.0",
    "firestore-store": "^1.0.0",
    "helmet": "^3.15.1",
    "lodash.throttle": "^4.1.1",
    "nuxt-purgecss": "^0.2.1",
    "nuxt-start": "^2.4.5",
    "nuxt-svg-loader": "^1.0.1",
    "portal-vue": "^1.5.1",
    "source-map-support": "^0.5.10",
    "tiny-cookie": "^1.0.1",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.6.7",
    "vue-i18n": "^8.9.0",
    "vue-meta": "^1.5.8",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.6.7",
    "vue-template-compiler": "^2.6.7",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-preset-minify": "^0.5.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0",
    "tailwindcss": "^0.7.4",
    "webpack": "^4.29.5"
  },
  "private": false
}
